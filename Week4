#include<stdio.h>
//while loop

#include<stdio.h>
int main()
{
int i,times;
scanf("%d",&times);
i = 0;
while (i <= times)
{
i++;
printf("%d\n",i);
}
return 0;
}



#include<stdio.h>
int main()
{
int i, times;
scanf("%d",&times);
i = times;
while (i >=0)
{
i--;
printf("%d\n",i);
}
return 0;
}



#include<stdio.h>
int main()
{
int i,times;
scanf("%d",&times);
i = 0;
do
{
i++;
printf("%d\n",i);
}
while (i <= times);
return 0;
}



//for loop

#include<stdio.h>
int main()
{
int i,times;
scanf("%d",&times);
for( i=0;i<times;i++)
{
printf("%d\n",i);
}
return 0;
}


#include<stdio.h>
int main()
{
int i,times;
scanf("%d",&times);
for(i=times;i>0;i--)
{
printf("%d\n",i);
}
return 0;
}



//nested for loop
#include<stdio.h>
int main()
{
int i;
for (i=1; i<=9; i++)
{
printf("\n");
for (int j=1; j<=i; j++)
{
printf("%d", j);
}
}
printf("\n");
return 0;
}


#include<stdio.h>
int main()
{
int i;
for (i=1; i<=3; i++)
{
printf("\n");
for (int j=1; j<=10; j++)
{
printf("%d X %d = %d\n", i, j, i*j);
}
}
printf("\n");
return 0;
}
